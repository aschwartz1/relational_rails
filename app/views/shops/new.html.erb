<form action="/shops" method="post">
  <input type="hidden" name="authenticity_token" value="<% form_authenticity_token %>">
  <p>Enter a new shop:</p>
  <input type='text' name='shop[name]' /></br>
  <input type='checkbox' name='shop[liquor_license]' value="License"/></br>
  <label for='shop[liquor_license]'>Does this shop have a liquor license?</label></br>
  <input type='submit'/>
</form>
